{
  "name": "medilinka-backend",
  "version": "1.0.0",
  "description": "MediLinka Telemedicine & Telepharmacy Backend API",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "test": "jest",
    "migrate": "npx sequelize-cli db:migrate",
    "seed": "npx sequelize-cli db:seed:all",
