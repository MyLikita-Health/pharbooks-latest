{
  "name": "medilinka-server",
  "version": "1.0.0",
  "description": "MediLinka Healthcare Management System - Backend Server",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "migrate": "npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "seed:undo": "npx sequelize-cli db:seed:undo:all",
    "reset-db": "npm run migrate:undo && npm run migrate && npm run seed",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "keywords": ["healthcare", "medical", "appointment", "telemedicine", "nodejs", "express", "websocket"],
  "author": "MediLinka Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "helmet": "^7.1.0",
    "express-rate-limit": "^7.1.5",
    "bcryptjs": "^2.4.3",
    "jsonwebtoken": "^9.0.2",
    "sequelize": "^6.35.2",
    "sqlite3": "^5.1.6",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.7",
    "ws": "^8.14.2",
    "uuid": "^9.0.1",
    "joi": "^17.11.0",
    "moment": "^2.29.4",
    "dotenv": "^16.3.1"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "jest": "^29.7.0",
    "supertest": "^6.3.3",
    "eslint": "^8.55.0"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/medilinka/server.git"
  },
  "bugs": {
    "url": "https://github.com/medilinka/server/issues"
  },
  "homepage": "https://github.com/medilinka/server#readme"
}
